
@{
    ViewBag.Title = "PickupDailyDealOrder";
}

@using SmartStock.Core.Models
@using SmartStock.Core.Enums
@model DailyDealOrderSummary

@{
    LoginUser loginUser = (LoginUser)Session["CurrentLoginUser"];
}
<section class="content-header">
    <h1>
        交易挑选
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-gear"></i>数据</a></li>
        <li class="active">交易挑选</li>
    </ol>
</section>

<section class="content">
    <div class="row"></div>
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <i class="fa fa-list"></i><h3 class="box-title">交易挑选</h3>
                </div>
                <div class="box-body admin-content">
                    @Html.Partial("_PickupDDOrderAllPartial", Model.AllDailyDealOrders)
                </div>
                <div class="box-footer">
                    <a href="javascript:void(0)" class="btn btn-primary pull-right pickup-save-btn"><i class="fa fa-save"></i> 保存</a>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="~/Scripts/stock/dailydealordermanager.js"></script>

@section Scripts{
    <script type="text/javascript">

        $(document).ready(function () {
            initCommonDataTable('#DailyDealOrderDataTbl');
            bindPickupDailyDealOrderEvent();
        });

    </script>
}

